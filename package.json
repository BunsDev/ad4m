    "@smui/button": "^4.2.0",
    "@smui/paper": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@types/faker": "^5.5.7",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.0",
    "@types/js-yaml": "^4.0.2",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.11",
    "@types/secp256k1": "^4.0.3",
